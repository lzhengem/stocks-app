<tr>
    <td><%= stock.name %></td>
    <td><%= stock.price%></td>
        <!--Dividends-->
    <td><%= stock.dividends%></td>
    
    <!--REV score-->
    <td class="<%= stock.rev_score.nil? ? 'N/A' : stock.rev_score.downcase %>"><%= stock.rev_score %></td> 
    <!--EPS score-->
    <td class="<%= stock.eps_score.nil? ? 'N/A' : stock.eps_score.downcase %>"><%= stock.eps_score %></td> 

    
    <!--ROE score-->
    <td class="<%= stock.roe_score.nil? ? 'N/A' : stock.roe_score.downcase %>"><%= stock.roe_score %></td> 

    <!--Analyst Rec-->
    <td class="<%= stock.analyst_rec_score.nil? ?  'N/A' : stock.analyst_rec_score.downcase %>"><%= stock.analyst_rec_score %></td> 
    
    <!--Surprises-->
    <td class="<%= stock.surprises_score.nil? ? 'N/A' : stock.surprises_score.downcase %>"><%= stock.surprises_score %></td> 
    <!--earnings growth-->
    <td class="<%= stock.earnings_growth_score.nil? ? 'N/A' : stock.earnings_growth_score.downcase %>"><%= stock.earnings_growth_score %></td> 
    <!--Short interest-->
    <td class="<%= stock.short_interest_score.nil? ? 'N/A' : stock.short_interest_score.downcase %>"><%= stock.short_interest_score %></td> 
    <!--Insider Trading-->
    <td class="<%= stock.insider_trading_score.nil? ? 'N/A' : stock.insider_trading_score.downcase %>"><%= stock.insider_trading_score %></td> 
</tr>